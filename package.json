{
  "name": "electron-shadcn",
  "productName": "electron-shadcn Template",
  "version": "1.0.0",
  "description": "Electron Forge with shadcn-ui (Vite + Typescript)",
  "main": ".vite/build/main.js",
  "private": true,
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "format": "npx @biomejs/biome format --write .",
    "lint": "npx @biomejs/biome lint --write .",
    "check": "npx @biomejs/biome check --write .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:unit": "vitest run src",
    "test:integration": "vitest run test/integration",
    "test:e2e": "playwright test",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "author": "ROG <luan.roger.2003@gmail.com>",
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": "npx @biomejs/biome check --write"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@chromatic-com/storybook": "^4.1.1",
    "@electron-forge/cli": "^7.9.0",
    "@electron-forge/maker-deb": "^7.9.0",
    "@electron-forge/maker-rpm": "^7.9.0",
    "@electron-forge/maker-squirrel": "^7.9.0",
    "@electron-forge/maker-zip": "^7.9.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.9.0",
    "@electron-forge/plugin-fuses": "^7.9.0",
    "@electron-forge/plugin-vite": "^7.9.0",
    "@electron-forge/shared-types": "^7.8.1",
    "@electron/fuses": "~1.8.0",
    "@playwright/test": "^1.55.1",
    "@storybook/addon-a11y": "^9.1.10",
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/addon-onboarding": "^9.1.10",
    "@storybook/addon-vitest": "^9.1.10",
    "@storybook/react-vite": "^9.1.10",
    "@tanstack/router-plugin": "^1.132.51",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/node": "^24.7.0",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "electron": "^38.2.1",
    "electron-devtools-installer": "^4.0.0",
    "electron-playwright-helpers": "^1.8.2",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.2.6",
    "playwright": "^1.56.0",
    "storybook": "^9.1.10",
    "tailwindcss": "^4.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.9",
