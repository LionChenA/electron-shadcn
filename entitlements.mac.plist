<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <!--
      This entitlement allows the JIT compiler to work, which is required for V8 to run with optimal performance.
      Hardened Runtime requires this to be explicitly enabled.
    -->
    <key>com.apple.security.cs.allow-jit</key>
    <true/>
  </dict>
</plist>
